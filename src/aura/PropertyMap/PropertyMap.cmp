<aura:component implements="flexipage:availableForAllPageTypes" access="global">

  <aura:attribute name="vfHost" type="String" />
  <aura:attribute name="record" type="Object" />
  <!-- fields="Id,Name,Street,City,State,Country,PostalCode,Latitude,Longitude" -->
  <aura:attribute name="kmlLayers" type="Object[]" />
  <aura:attribute name="test" type="String" default="PropertyMap test"/>

  <aura:handler name="init" value="{! this }" action="{! c.doInit }" />

  <aura:registerEvent name="updateAttributes" type="c:PropertyAttributeUpdate" />

  <p>PropertyMap.cmp Testing. !v.record.Name:</p>
  <p>{!v.record.Name}</p>

  <iframe aura:id="vfFrame" src="{! 'https://' + v.vfHost + '/apex/PropertyMap' }"
    frameborder="0" width="100%" height="376" />

</aura:component>
